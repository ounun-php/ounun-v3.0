{
  "name": "ounun-php/core-v3.0",
  "description": "the new ounun framework",
  "type": "library",
  "keywords": [
    "ounun",
    "ounun.org"
  ],
  "homepage": "https://www.ounun.org/",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-bcmath": "*",
    "ext-zlib": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-memcached": "*",
    "ext-redis": "*",
    "ext-gd": "*",
    "ext-xml": "*",
    "ext-xmlrpc": "*",
    "ext-simplexml": "*",
    "ext-iconv": "*",
    "php": "7.4.*",
    "ext-fileinfo": "*",
    "ext-imagick": "*"
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "sebastian/version": "^3.0",
    "phpunit/phpunit": "^9.2"
  },
  "autoload": {
    "psr-4": {
      "ounun\\": "ounun"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": []
  }
}
