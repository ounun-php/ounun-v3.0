  "authors": [],
  "require": {
    "ext-json": "*",
    "ext-curl": "*",
    "ext-ftp": "*",
    "ext-bcmath": "*",
    "ext-zlib": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-xml": "*",
    "ext-fileinfo": "*",
    "ext-ctype": "*",
    "ext-ldap": "*",
    "ext-sockets": "*",
    "ext-simplexml": "*",
    "ext-xmlrpc": "*",

    "textalk/websocket": "^1.2",
    "php": ">=7.2",
    "ext-apcu": "*"
